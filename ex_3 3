x = 3.14159
y = 1.123
print (f'hello world x = {x: 5f} ' )
print (f'hello world x = {y: 5f} ' )

username = input("Enter username:")

print("Username is: " + username)

x = float(input("Enter float x:"))
print(f'Hello World x = {x: 5f} ')

x = int(input("Enter int x:"))
print (f'Hello World x = {x:5d} ')
